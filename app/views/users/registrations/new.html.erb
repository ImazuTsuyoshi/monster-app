<div class="sessions_new">
<h2>応募フォーム</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "users/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :phone %><br />
    <%= f.text_field :phone, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :history %><br />
    <%= f.text_field :history, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :uniform %><br />
    <%= f.text_field :uniform, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :position %><br />
    <%= f.text_field :position, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :self_introduction %><br />
    <%= f.text_area :self_introduction, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> 文字以上)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <p><div class="actions">
    <%= f.submit "送信" %>
  </div></p>
<% end %>

<%= render "users/shared/links" %>
</div>
